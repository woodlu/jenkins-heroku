#!groovy
// Check ub1 properties
properties([disableConcurrentBuilds()])

pipeline {
    agent any
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '3'))
        timestamps()
    }
    stages {
        stage("First step") {
            steps {
                sh 'docker-compose up'
            }
        }
    }
}
